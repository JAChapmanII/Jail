#include <sstream>
template<typename T>
        void config::set(string scoped_variable, T value) {
    config::init();
    config::map.set(scoped_variable, value);
}
template<typename T>
        void config::set(string scope, string variable, T value) {
    config::init();
    config::map.set(scope, variable, value);
}

